{
  "name": "jore-map-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/leaflet": "^1.2.7",
    "@types/proj4": "^2.3.4",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "apollo-boost": "^0.1.10",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "leaflet": "^1.3.1",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "proj4": "^2.4.4",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-react": "^3.6.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "watch-css": "npm run build-css && sass --watch src/styles/scss:src/styles/css",
    "start-js": "react-scripts-ts start",
    "build": "npm-run-all build-css build-js",
    "build-css": "sass --update src/styles/scss:src/styles/css",
    "build-js": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "lint-fix": "tslint --fix --project tsconfig.json --format codeFrame 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.1",
    "mobx-react-devtools": "^6.0.1",
    "npm-run-all": "^4.1.3",
    "sass": "^1.10.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2"
  }
}
